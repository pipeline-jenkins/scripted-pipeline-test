node {
    stage('checkout') {       
        echo 'Checking out'   
	    Thread.sleep(500)
    }
	
	stage('Build') {
        echo 'Building'
		Thread.sleep(500)
    }
	
	stage('test') {
        echo 'Running tests'
		Thread.sleep(500)
    }
	
	stage('package') {
        echo 'Packaging artifacts'
		Thread.sleep(500)
    }
	
	stage('deploy') {
        echo 'Deploy'
		Thread.sleep(500)
    }
}